<?xml version="1.0" encoding="UTF-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Vanilla Furniture Expanded - Production</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>

      <li Class="PatchOperationAdd">
        <xpath>/Defs/ThingDef[
        @Name="20mmProgrammableGrenadeBase" or
        @Name="107mmRocketBase" or
        @Name="120mmRocketBase" or
        @Name="122mmGuidedHowitzerShellBase"
        ]/tradeTags</xpath> 
        <value>
          <li>CE_AutoEnableCrafting_VFE_TableMachiningLarge</li>
        </value>
      </li>
      </operations>
    </match>
  </Operation>
</Patch>